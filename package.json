{
  "name": "create-react-node-app",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "lint": "eslint src",
    "webpack:build:server": "webpack --env.server",
    "webpack:watch:frontend": "webpack-dev-server --env.frontend --hot --open",
    "dev-server": "nf run npm run webpack:build:server && nf start -j Procfile-dev",
    "dev-front": "nf run npm run webpack:watch:frontend",
    "dev": "concurrently \"npm run dev-server\" \"sleep 5 && npm run dev-front\"",
    "test": "nf run NODE_ENV=test sequelize db:migrate:undo:all && nf run NODE_ENV=test sequelize db:migrate && nf run NODE_ENV=test sequelize db:seed:all && nf run NODE_ENV=test jest -- --config ./jest-config.json",
    "build": "webpack",
    "prod": "node dist/index.js",
    "db:migration:create": "nf run sequelize migration:create",
    "db:migrate": "nf run sequelize db:migrate",
    "db:migrate:undo": "nf run sequelize db:migrate:undo",
    "db:migrate:undo:all": "nf run sequelize db:migrate:undo:all",
    "db:seed:create": "nf run sequelize seed:create",
    "db:seed:all": "nf run sequelize db:seed:all",
    "db:seed:undo": "nf run sequelize db:seed:undo -- --seed",
    "db:seed:undo:all": "nf run sequelize db:seed:undo:all",
    "flow": "flow",
    "flow:types:install": "flow-typed install --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/derniercri/create-react-node-app.git"
  },
  "author": "DernierCri <bonjour@derniercri.io>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/derniercri/create-react-node-app/issues"
  },
  "homepage": "https://github.com/derniercri/create-react-node-app#readme",
  "engines": {
    "node": "8.2.1",
    "npm": "5.3.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^18.0.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.0",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.4",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "foreman": "^2.0.0",
    "ioredis": "^3.1.1",
    "node-mailjet": "^3.1.0",
    "node-sass": "^4.5.3",
    "pg": "^7.0.2",
    "prop-types": "^15.5.10",
    "pug": "^2.0.0-rc.2",
    "raven": "^2.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.7.1",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.4.2",
    "sequelize-cli": "^2.7.0",
    "superagent": "^3.5.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.51.0",
    "flow-typed": "^2.1.5",
    "jest": "^18.0.0",
    "nodemon": "^1.11.0",
    "regenerator-runtime": "^0.10.5",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
