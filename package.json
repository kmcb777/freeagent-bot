{
  "name": "pmdc",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "test": "jest --config ./jest-config.json",
    "build": "nf run babel-node scripts/bundle.js -j Procfile-dev",
    "lint": "eslint src",
    "dev-server": "nf start -j Procfile-dev",
    "dev-front": "nf run webpack-dev-server -- --hot --open",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-front\"",
    "db:migration:create": "nf run sequelize migration:create",
    "db:migrate": "nf run sequelize db:migrate",
    "db:migrate:undo": "nf run sequelize db:migrate:undo",
    "db:migrate:undo:all": "nf run sequelize db:migrate:undo:all",
    "db:seed:create": "nf run sequelize seed:create",
    "db:seed:all": "nf run sequelize db:seed:all",
    "db:seed:undo": "nf run sequelize db:seed:undo -- --seed",
    "db:seed:undo:all": "nf run sequelize db:seed:undo:all"
  },
  "repository": {
    "type": "git",
